<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.holdsafetyadmin">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HoldSafetyAdmin">
        <activity android:name=".RejectUserActivity"></activity>
        <activity
            android:name=".RegistrationDetailsActivity"
            android:label="Details"
            android:parentActivityName=".ValidationListActivity" />
        <activity android:name=".ValidationListActivity" />
        <activity android:name=".LandingActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>